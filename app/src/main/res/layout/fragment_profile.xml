<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".navigation.ProfileFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".navigation.ProfileActivity">

        <TextView
            android:id="@+id/usrNameTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:layout_marginTop="25dp"
            android:fontFamily="@font/sen_bold"
            android:text="@string/your_name"
            android:textColor="#000000"
            android:textSize="20sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/usrPhoneTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:fontFamily="@font/sen"
            android:text="@string/phone"
            android:textSize="16sp"
            app:layout_constraintStart_toStartOf="@+id/usrNameTextView"
            app:layout_constraintTop_toBottomOf="@+id/usrNameTextView" />

        <TextView
            android:id="@+id/partnerTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:drawableLeft="@drawable/credit_card"
            android:drawablePadding="17dp"
            android:fontFamily="@font/sen"
            android:includeFontPadding="false"
            android:paddingStart="10dp"
            android:paddingTop="30dp"
            android:text="@string/partnerprofile"
            android:textAlignment="textStart"
            android:textColor="#000000"
            android:textSize="16sp"
            app:layout_constraintStart_toStartOf="@+id/usrPhoneTextView"
            app:layout_constraintTop_toBottomOf="@+id/usrPhoneTextView" />

        <TextView
            android:id="@+id/profileTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="-3dp"
            android:drawableLeft="@drawable/person"
            android:drawablePadding="10dp"
            android:fontFamily="@font/sen"
            android:paddingStart="10dp"
            android:paddingTop="25dp"
            android:textAlignment="center"
            android:textColor="#000000"
            android:textSize="16sp"
            app:layout_constraintStart_toStartOf="@+id/partnerTextView"
            app:layout_constraintTop_toBottomOf="@+id/partnerTextView" />

        <TextView
            android:id="@+id/settingsTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:drawableLeft="@drawable/gear"
            android:layout_marginStart="3dp"
            android:drawablePadding="20dp"
            android:fontFamily="@font/sen"
            android:paddingStart="10dp"
            android:paddingTop="30dp"
            android:text="@string/settings"
            android:textAlignment="center"
            android:textColor="#000000"
            android:textSize="16sp"
            app:layout_constraintStart_toStartOf="@+id/profileTextView"
            app:layout_constraintTop_toBottomOf="@+id/profileTextView" />

        <TextView
            android:id="@+id/aboutUsTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:drawableLeft="@drawable/info_circle"
            android:drawablePadding="20dp"
            android:fontFamily="@font/sen"
            android:paddingStart="10dp"
            android:paddingTop="30dp"
            android:text="@string/aboutus"
            android:textAlignment="center"
            android:textColor="#000000"
            android:textSize="16sp"
            app:layout_constraintStart_toStartOf="@+id/settingsTextView"
            app:layout_constraintTop_toBottomOf="@+id/settingsTextView" />

        <TextView
            android:id="@+id/helpTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:drawableLeft="@drawable/question_circle"
            android:drawablePadding="20dp"
            android:fontFamily="@font/sen"
            android:paddingStart="10dp"
            android:paddingTop="30dp"
            android:text="@string/help"
            android:textAlignment="center"
            android:textColor="#000000"
            android:textSize="16sp"
            app:layout_constraintStart_toStartOf="@+id/aboutUsTextView"
            app:layout_constraintTop_toBottomOf="@+id/aboutUsTextView" />

        <TextView
            android:id="@+id/secondProfileTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="4dp"
            android:layout_marginTop="25dp"
            android:fontFamily="@font/sen"
            android:text="@string/profile"
            android:textColor="#000000"
            android:textSize="16sp"
            app:layout_constraintBottom_toBottomOf="@+id/profileTextView"
            app:layout_constraintStart_toEndOf="@+id/profileTextView"
            app:layout_constraintTop_toTopOf="@+id/profileTextView" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>